<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.smartcarrest">

    <uses-feature
            android:name="android.permission.CAMERA"
            android:required="true"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Profile">
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".ChatDetail">
        </activity>
        <activity android:name=".CameraAcces">
        </activity>
        <activity android:name=".PertolonganPertama">
        </activity>
        <activity android:name=".ChatActivity">
        </activity>
        <activity android:name=".CallCenter">
        </activity>
        <activity android:name=".PenangananHentiJantung">
        </activity>
        <activity android:name=".AboutJantungHenti">
        </activity>
        <activity android:name=".Home">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>